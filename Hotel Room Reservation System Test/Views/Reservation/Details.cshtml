@model Hotel_Room_Reservation_System_Test.Models.Reservation

<h1>Reservation Details</h1>

<div>
    <dl class="row">
        <dt class="col-sm-2">Id</dt>
        <dd class="col-sm-10">@Model.Id</dd>
        <dt class="col-sm-2">RoomId</dt>
        <dd class="col-sm-10">@Model.RoomId</dd>
        <dt class="col-sm-2">UserId</dt>
        <dd class="col-sm-10">@Model.UserId</dd>
        <dt class="col-sm-2">CheckInDate</dt>
        <dd class="col-sm-10">@Model.CheckInDate</dd>
        <dt class="col-sm-2">CheckOutDate</dt>
        <dd class="col-sm-10">@Model.CheckOutDate</dd>
        <dt class="col-sm-2">Status</dt>
        <dd class="col-sm-10">@Model.Status</dd>
    </dl>
</div>
